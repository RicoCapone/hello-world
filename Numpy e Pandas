    "#creo la seconda matrice 5x4 e utilizzo il costrutto fromfunction\n",
    "def fun(row_num, col_num):\n",
    "    return 2*row_num + pow(col_num,1/3)\n",
    "\n",
    "F = np.fromfunction(fun,(5, 4), dtype=float)\n",
    "F"
   ]
